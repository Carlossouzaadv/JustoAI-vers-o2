{
  "name": "justoai-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:web": "next dev",
    "build": "next build",
    "build:web": "next build",
    "start": "next start",
    "start:web": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --maxWorkers=2",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "install:monitoring": "node scripts/install-process-monitoring.js",
    "monitor:start": "node scripts/start-monitoring.js",
    "monitor:stop": "node scripts/stop-monitoring.js",
    "workers:start": "node scripts/start-workers.js start",
    "workers:stop": "node scripts/start-workers.js stop",
    "workers:restart": "node scripts/start-workers.js restart",
    "workers:status": "node scripts/start-workers.js status",
    "workers:logs": "node scripts/start-workers.js logs",
    "worker:judit": "node --loader tsx/esm src/workers/juditOnboardingWorker.ts",
    "worker:judit:dev": "npx tsx src/workers/juditOnboardingWorker.ts",
    "worker:judit:pm2": "pm2 start --no-autorestart --name judit-worker --interpreter node -- --loader tsx/esm src/workers/juditOnboardingWorker.ts",
    "start:worker": "node --loader tsx/esm src/workers/juditOnboardingWorker.ts",
    "optimize:images": "node scripts/optimize-images-sharp.js",
    "analyze:performance": "node scripts/analyze-performance.js",
    "build:optimized": "npm run optimize:images && npm run build",
    "performance:full": "npm run build:optimized && npm run analyze:performance",
    "test:responsive": "node scripts/test-responsiveness.js",
    "test:mobile": "BASE_URL=http://localhost:3000 node scripts/test-responsiveness.js",
    "docs:generate": "swagger-jsdoc -d swagger.config.json src/app/api/**/*.ts src/lib/swagger-docs/**/*.ts -o swagger.json",
    "docs:serve": "swagger-ui-serve swagger.json",
    "docs:dev": "npm run dev",
    "deploy:check": "node deploy-scripts/01-pre-deploy-check.js",
    "deploy:test": "node deploy-scripts/04-test-connections.js",
    "deploy:prod": "npm run deploy:check && npm run deploy:test && vercel --prod",
    "deploy:preview": "npm run deploy:check && vercel",
    "check:imports": "madge --circular --extensions ts,tsx src/",
    "check:all": "npm run type-check && npm run lint && npm run check:imports",
    "verify:worker": "npx tsx scripts/verify-worker-config.ts",
    "verify:worker:local": "NODE_ENV=development npx tsx scripts/verify-worker-config.ts",
    "postinstall": "node scripts/postinstall.js",
    "extract:help-content": "node scripts/extract-help-content.js",
    "sync:crisp-kb": "node scripts/sync-crisp-kb.js"
  },
